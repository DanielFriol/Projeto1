@model AvaliationAddVM
@{
    ViewBag.Title = "Avaliar Ônibus";



}

<h2>@ViewBag.Title</h2>
<form method="post">
    <div class="form-check form chek-inline">
        <input class="form-check-input" type="checkbox" id="cb1" value=1 />
        <label class="form-check-label" for="cb1">1</label>
    </div>
    <div class="form-check form chek-inline">
        <input class="form-check-input" type="checkbox" id="cb2" value=2 />
        <label class="form-check-label" for="cb2">2</label>
    </div>
    <div class="form-check form chek-inline">
        <input class="form-check-input" type="checkbox" id="cb3" value=3 />
        <label class="form-check-label" for="cb3">3</label>
    </div>
    <div class="form-check form chek-inline">
        <input class="form-check-input" type="checkbox" id="cb4" value=4 />
        <label class="form-check-label" for="cb4">4</label>
    </div>
    <div class="form-check form chek-inline">
        <input class="form-check-input" type="checkbox" id="cb5" value=5 />
        <label class="form-check-label" for="cb5">5</label>
    </div>

    <button type="submit" class="btn btn-outline-dark">Enviar</button>
    <a asp-action="Index" asp-controller="Avaliations" class="btn btn-info">Cancelar</a>

</form>
@section scripts{

    <script src="~/node_modules/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/node_modules/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.js"></script>

}


